pipeline {

stages {
    stage('Git_Checkout') {
    agent any
    steps{
     git url: 'https://github.com/abhisheknipun1/DevOps-Project.git'
    }
  }
    stage('Build') {
        agent { //here we select only docker build agents
            docker {
                image 'maven:latest' //container will start from this image
            
            }
             steps {
            sh 'mvn clean package' //this command will be executed inside maven container
        }
    }
    }
    }
    }
